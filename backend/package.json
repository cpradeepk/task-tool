{
  "name": "task-tool-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Task management backend (Node.js, Express, Socket.io, BullMQ, PostgreSQL)",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "lint": "eslint .",
    "test": "node -e \"console.log('No tests yet')\"",
    "migrate:latest": "knex migrate:latest --knexfile src/db/knexfile.cjs",
    "migrate:rollback": "knex migrate:rollback --knexfile src/db/knexfile.cjs",
    "migrate:make": "knex migrate:make --knexfile src/db/knexfile.cjs",
    "seed:run": "knex seed:run --knexfile src/db/knexfile.cjs",
    "seed:make": "knex seed:make --knexfile src/db/knexfile.cjs"
  },
  "dependencies": {
    "bullmq": "^5.7.8",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.13",
    "socket.io": "^4.7.5",
    "knex": "^3.1.0",
    "objection": "^3.1.4",
    "pg": "^8.12.0",
    "google-auth-library": "^9.14.2",
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/s3-request-presigner": "^3.637.0",
    "exceljs": "^4.4.0",
    "pdfkit": "^0.15.0",
    "ics": "^3.7.6"
  },
  "devDependencies": {
    "eslint": "^9.8.0",
    "nodemon": "^3.1.4"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}

